clear all;
close all;
clc

%% STATIC



%% DYNAMIC

    Kll = Kg(vl,vl);
    Mll = Mg(vl,vl);
    [phi,lambda] = eigs(Kll,Mll);
    lambda = sqrt(diag(lambda));
    freq = sqrt(lambda)/2/pi;
    
    % Direct method
    X_dir = direct(Kg, Mg, F, vl, lambda);
    % Modal method (2 and 1 eigenvalues)
    X_mod = modal(Kg, Mg, F, vl, phi, lambda);


disp('Finish');



